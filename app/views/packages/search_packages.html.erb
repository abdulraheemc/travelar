<h4><div class= "row">Starting City:<%= params[:from]%> ||
Destination:<%= params[:destination]%></div></h4>
<table class="table">
<thead>
<tr>
  <th>Package Name</th>
  <th>Package Price</th>
  <th>Package Duration</th>
  <th> </th>
  <th colspan="3"></th>
   </tr>
  </thead>
 <tbody>
    
  <% @package.each do |package| %>
  <tr>
    <td> <%= package.name %></td>
    <td>  <%= package.price %></td>
    <td><%= package.duration %></td>
    <td> <%= link_to 'Book Now', pilgrim_new_path(package), class: "btn btn-primary"  %>
  </tr> 
  <% end %>
  </tbody>
</table>