<table class="table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Middle Name</th>
      <th>Last Name</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pilgrims.each do |pilgrim| %>
      <tr>
        <td><%= pilgrim.firstname %></td>
        <td><%= pilgrim.middlename %></td>
        <td><%= pilgrim.lastname %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

